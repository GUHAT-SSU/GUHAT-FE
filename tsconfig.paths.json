{
    "isolatedModules": false,
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@apis/*": ["src/apis/*"],
            "@assets/*": ["src/assets/*"],
            "@components/*": ["src/components/*"],
            "@constants/*": ["src/constants/*"],
            "@pages/*": ["src/pages/*"],
            "@public/*": ["public/*"],
            "@lib/*": ["src/lib/*"],
            "@_types/*": ["src/types/*"]
        }
    }
}
